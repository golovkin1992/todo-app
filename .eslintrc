 {
    "extends": ["airbnb", "plugin:jsx-a11y/recommended"],
    "plugins": ["jsx-a11y"],
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "rules":{
      "linebreak-style": 0,
      "jsx-a11y/label-has-for": [ 2, {
        "components": [ "Label" ],
        "required": {
            "some": [ "nesting", "id" ]
        },
        "allowChildren": false
    }]
      
    }

}